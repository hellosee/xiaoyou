.bg-main-init{
    font-size: 26rpx;
    background-color: #f1f2f4;
}
.members-wrapper {
    padding: 16rpx;
}
.members-inner {
    width: 100%;
    height: 70rpx;
    border-radius: 8rpx;
    background-color: #fff;
}
.members-icon {
    float: left;
    width: 40rpx;
    height: 100%;
    margin-left: 50rpx;
    background: url(https://fonehui.com/static/img/wximg/search.png) center no-repeat;
    background-size: 40rpx;
}
.members-input-wrapper {
    margin-left: 100rpx;
}
.members-input {
    width: 90%;
    font-size: 26rpx;
    line-height: 70rpx;
}
input[readonly] {
    color: #898b8e;
}
.all-members-wrapper {
    margin-top: 20rpx;

    background-color: #fff;
    width:100%;
}
.all-members-header {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    color: #9ea1a4;
}
.all-members-header .h2 {
    font-size: 32rpx;
    padding-left: 30rpx;
    font-weight: normal;
}

.all-member-list {
    padding: 30rpx 30rpx 30rpx 30rpx;
}
.border-top {
    border-top: 2rpx solid #f1f2f4;
    font-size: 28rpx;
}
.all-member-portrait {
    float: left;
    width: 100rpx;
    height: 100rpx;
}
.member-avatar {
    width: 100%;
    height: 100%;
    border-radius: 10rpx;
}
.all-member-info {
    margin-top: 6rpx;
    margin-left: 120rpx;
}
.all-member-name {
    margin-top: -6rpx;
    font-size: 32rpx;
    color: #343536;
}
.all-member-position {
    min-height: 32rpx;
    font-size: 24rpx;
    color: #9ea1a4;
}